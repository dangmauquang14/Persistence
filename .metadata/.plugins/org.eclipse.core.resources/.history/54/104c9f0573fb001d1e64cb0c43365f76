package sort;

public class BubbleSort {

}

private static void sort(int[] numbers) {
	for(int round = 0; round < numbers.length; round ++) {
		for(int i = 0; i < numbers.length - round - 1; i++) {
			if(numbers[i] > numbers[i + 1]) {
				SortUtils.swap(numbers, i, i+1);
			}
		}
	}
}